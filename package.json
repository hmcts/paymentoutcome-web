  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production ./node_modules/.bin/ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "NODE_OPTIONS=--openssl-legacy-provider nodemon --config nodemon.json",
    "lint": "NODE_OPTIONS=--openssl-legacy-provider tslint -p tsconfig.json -p && sass-lint -v -q",
    "build": "NODE_OPTIONS=--openssl-legacy-provider  yarn webpack --config webpack.config.js",
    "build:prod": "NODE_OPTIONS=--openssl-legacy-provider  NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "test": "NODE_OPTIONS=--openssl-legacy-provider jest",
    "test:coverage": "NODE_OPTIONS=--openssl-legacy-provider jest --coverage",
    "test:routes": "NODE_OPTIONS=--openssl-legacy-provider jest -c jest.routes.config.js",
    "test:a11y": "NODE_OPTIONS=--openssl-legacy-provider jest -c jest.a11y.config.js --maxWorkers 15",
    "sonar-scan": "NODE_OPTIONS=--openssl-legacy-provider  sonar-scanner",
    "test:smoke": "NODE_OPTIONS=--openssl-legacy-provider  NODE_TLS_REJECT_UNAUTHORIZED=0 && jest -c jest.smoke.config.js",
    "test:functional": "NODE_OPTIONS=--openssl-legacy-provider NODE_TLS_REJECT_UNAUTHORIZED=0 && jest -c jest.smoke.config.js",
    "fortifyScan": "./test/java/gradlew -p test/java fortifyScan",
    "cichecks": "NODE_OPTIONS=--openssl-legacy-provider yarn && yarn build && yarn lint  && yarn test && yarn test:routes &&  yarn test:a11y",
    "dependency-setup-with-yarn-lock": "NODE_OPTIONS=--openssl-legacy-provider yarn install --immutable"
  },
